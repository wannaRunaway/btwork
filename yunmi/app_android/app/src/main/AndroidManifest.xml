<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.btkj.millingmachine">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.STATUS_BAR"/>
    <uses-permission android:name="android.permission.EXPAND_STATUS_BAR"/>

    <application
        android:name=".homepage.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/selfcult_press"
        android:label="@string/app_name"
        android:roundIcon="@drawable/selfcult_press"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".homepage.MainActivity" android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".homepage.BaseActivity" />
        <activity android:name=".selftcult.SelfcultActivity" />
        <activity android:name=".selftcult.PaymentActivity" />
        <activity android:name=".help.HelpActivity" />
        <activity android:name=".rechargequery.AccountActivity" />
        <activity android:name=".rechargequery.AccountInquireActivity" />
        <activity android:name=".rechargequery.RechargeFinishActivity" />
        <activity android:name=".rechargequery.RechargeQueryActivity" />
        <activity android:name=".rechargequery.RechargeWayActivity" />
        <activity android:name=".ricetake.TakeRiceActivity" />
        <activity android:name=".selftcult.PayResultActivity"/>
        <activity android:name=".selftcult.SelfcultFinishActivity" />
        <activity android:name=".errorview.ConnectErrorActivity" />
        <activity android:name=".errorview.FixActivity" />
        <activity android:name=".errorview.MarginLackActivity" />
        <activity android:name=".errorview.UpgradeActivity" />
        <activity android:name=".homepage.SerialPortActivity" />
        <activity android:name=".rechargequery.RechargeCardPhoneActivity" />
    </application>
</manifest>